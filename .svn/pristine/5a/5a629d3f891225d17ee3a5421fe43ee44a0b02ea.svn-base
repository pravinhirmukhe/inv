    color:#F2B33F;
  
}
.sidebar-nav > ul > li > ul{padding-left:15px;}
.nav>li>a:hover, .nav>li>a:focus{background: transparent !important;}
.nav.main-menu > li.active > a{
    background:none;
    color:#F2B33F;
    /*border-right:4px solid #349886;*/
}
