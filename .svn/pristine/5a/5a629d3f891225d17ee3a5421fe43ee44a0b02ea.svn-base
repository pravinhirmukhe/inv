root { 
    display: block;
}
/*top taheader*/
a.navbar-brand span{
    color:#fff;
}
a#main-menu-act i{
    left:16%;
    bottom:-26px;
    font-size:20px;
}
a#main-menu-act{
    margin:0 0;
}
